			min-height: 100px;
		}

		textarea_div {
			background-color: var(--color-bg);
			resize: none;
			width: 63em;
			font-size: 0.76em;
			border: 1.8px solid var(--color-border);
			padding-left: 0.6em;
			padding-top: 0.5em;
			min-height: 100px;
			overflow-x: hidden;
			overflow-y: auto;

		}

		[contentEditable=true]:empty:not(:focus):before {
			content: attr(data-text);
