		}

		nav a {
			text-decoration: none;
			color: var(--color-link);
			padding: 10px 20px;
			position: relative;
			transition: all 0.3s ease;
		}

		nav a:hover {
			text-decoration: none;
		}

		nav a::before {
			content: "";
			position: absolute;
			bottom: 0;
			left: 0;
			width: 0;
			height: 3px;
			background-color: gray;
			transition: width 0.3s ease;
		}

		nav a:hover::before {
			width: 100%;
		}

		.divider {
