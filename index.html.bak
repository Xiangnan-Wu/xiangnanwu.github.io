				} else {
					if ($('.ab' + $(this).attr("id")).css("display") != "none") {
						$('.ab' + $(this).attr("id")).css("display", "none");
					}
					$('.bib' + $(this).attr("id")).css("display", "block");
				}
				const textArea = document.getElementById('mybib' + $(this).attr("id"));
				textArea.style.height = "auto";
